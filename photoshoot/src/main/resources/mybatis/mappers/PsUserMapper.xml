    <select id="selectPsUserById" parameterType="java.lang.Integer" resultMap="PsUserResultMap">
        select
        *
        from ps_user
        where ps_user.user_id = #{id}
    </select>

    <select id="selectPsUserByName" parameterType="java.lang.String" resultMap="PsUserResultMap">
        SELECT
            *
        FROM
            ps_user
        WHERE
	  ps_user.user_name =#{userName}
    </select>




    <insert id="insertPsUser" parameterType="PsUser">
        INSERT INTO ps_user (user_name, user_password)
        VALUES
	    (#{userName},#{userPassword})
    </insert>

